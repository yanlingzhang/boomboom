@import "../../common/base.wxss";
Page{
    background: #f7f7f7;
}
.icon-success{
    width: 100rpx;
    height: 100rpx;
    display: inline-block;
}
.money-info{
    font-size:36rpx;
    position: relative;
    top: -30rpx;
    left: 12rpx;
    display: inline-block;
}
.coupon-info{
    text-align: center;
    color: #fff;
    width: 750rpx;
    background: #fff;
    padding: 45rpx 60rpx 56rpx;
    font-size:22rpx;
    margin: 20rpx auto 0;
}
.coupon-zone{
    background: url('https://www.weiyoutong.cn/wyttest/Public/Home/images/boom/pay_icon_coupon.png') no-repeat;
    background-size: 100% 100%;
    height: 129rpx;
}
.coupon-price{
    font-size: 32rpx;
    padding-top: 26rpx;
    margin-bottom: 8rpx;
}
.coupon-price text{
    font-size: 30rpx;
    margin-left: 10rpx;
}
.pay-info{
    background: #e52845;
    padding: 52rpx 0;
    text-align: center;
    color: #fff;
}
.btn-zone{
    width: 750rpx;
    background: #fff;
    margin-top: 20rpx;
}
.btn-cell{
    width: 375rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: #fff;
    color: #777;
    box-sizing: border-box;
    text-align: center;
    font-size:32rpx;
}
.btn-cell:nth-child(1){
    border-right: 1px solid #f2f2f2;
}