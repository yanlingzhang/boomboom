@import "../../common/base.wxss";
Page{
    background: #fff;
}
.container{
    height: 100%;
}
.brand-nav{
    width: 18%;
    left: 0;
    height: 100%;
    position: absolute;
}
.nav-cell{
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid #bbbbbb;
    font-size: 24rpx;
}
.nav-cell.active{
    background: #000;
    color: #fff;
}
.brand-list{
    width: 80%;
    right: 0;
    padding: 1%;
    box-sizing: border-box;
    height: 100%;
    position: absolute;
}
.brand-cell{
    float: left;
    width: 32%;
    margin-right: 1%;
    text-align: center;
    height: 198rpx;
    line-height: 198rpx;
    color: #ffffff;
    margin-bottom: 1%;
    background: #000000;
}
